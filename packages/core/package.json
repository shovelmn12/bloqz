{
  "name": "@bloqz/core",
  "version": "1.0.3",
  "description": "Core package for the functional Bloqz state management library.",
  "keywords": [
    "bloc",
    "bloqz",
    "state management",
    "functional",
    "rxjs",
    "typescript",
    "core"
  ],
  "homepage": "https://github.com/shovelmn12/bloqz#readme",
  "bugs": {
    "url": "https://github.com/shovelmn12/bloqz/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shovelmn12/bloqz.git"
  },
  "license": "MIT",
  "author": "Shovel Maman",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.d.ts",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build:js": "NODE_ENV=production rolldown -c rolldown.config.ts && find dist -name '*.map' -delete",
    "build:types": "dts-bundle-generator -o dist/index.d.ts src/index.ts",
    "build": "pnpm prebuild && pnpm build:js && pnpm build:types",
    "prepublishOnly": "npm run build",
    "publish": "npm publish",
    "test": "vitest --run"
  },
  "dependencies": {
    "fp-ts": "^2.16.9",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "dts-bundle-generator": "^9.5.1",
    "rolldown": "^1.0.0-beta.44"
  }
}
