{
  "name": "@bloqz/core",
  "version": "1.0.3",
  "description": "Core package for the functional Bloqz state management library.",
  "keywords": [
    "bloc",
    "bloqz",
    "state management",
    "functional",
    "rxjs",
    "typescript",
    "core"
  ],
  "homepage": "https://github.com/shovelmn12/bloqz#readme",
  "bugs": {
    "url": "https://github.com/shovelmn12/bloqz/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shovelmn12/bloqz.git"
  },
  "license": "MIT",
  "author": "Shovel Maman",
  "type": "module",
  "exports": {
    "main": "./dist/index.js",
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build:js": "rolldown -c 'rolldown.config.ts' -m --minify-internal-exports",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "build": "pnpm prebuild && pnpm build:js && pnpm build:types",
    "prepublishOnly": "npm run build",
    "publish": "npm publish",
    "test": "vitest --run"
  },
  "dependencies": {
    "fp-ts": "^2.16.9",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "rolldown": "^1.0.0-beta.44"
  }
}
